{
	"_version": "1.7.0",
	"sap.app": {
		"id": "ui.pmc.pricing",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "ui.pmc.pricing.view.PricingMainView",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ui.pmc.pricing.i18n.i18n"
				}
			},
			"purchaseitems": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/purchase.json"
			},
			"tableItems":{
			    "type":"sap.ui.model.json.JSONModel",
				"uri":"model/TablePayload.json"
			},
			"medData": {
			"type": "sap.ui.model.json.JSONModel",
			"settings": {},
			"preload": false
			},
			"AddEditData": {
				"type": "sap.ui.model.json.JSONModel"

			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "Z_RateCard_pricing.view",
				"controlId": "appId",
				"controlAggregation": "pages"
			},
			"routes": [{
				"name": "RateCard",
				"pattern": "RateCard",
				"target": "RateCard"
			}, {
				"name": "RateCard_Detail",
				"pattern": "RateCard_Detail",
				"target": "RateCard_Detail"
			}, {
				"name": "NewRateCard",
				"pattern": "NewRateCard",
				"target": "NewRateCard"
			}],
			"targets": {
				"RateCard": {
					"viewType": "XML",
					"viewName": "RateCard"
				},
				"RateCard_Detail": {
					"viewType": "XML",
					"viewName": "Pricing_SubView_RateCardDetail"
				},
				"NewRateCard": {
					"viewType": "XML",
					"viewName": "NewRateCard"
				}
			}
		}
	}
}